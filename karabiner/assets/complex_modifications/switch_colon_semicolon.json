{
  "title": "Switch colon and semicolon",
  "rules": [
                    {
                        "description": "Switch colon and semicolon",
                        "manipulators": [
                            {
                                "from": {
                                    "key_code": "semicolon"
                                },
                                "to": [
                                    {
                                        "key_code": "semicolon", "modifiers": "shift"
                                    }
                                ],
                                "type": "basic"
                            },
                            {
                                "from": {
                                    "key_code": "semicolon",
                                    "modifiers": {
                                        "mandatory": [
                                            "shift"
                                        ]
                                    }
                                },
                                "to": [
                                    {
                                        "key_code": "semicolon"
                                    }
                                ],
                                "type": "basic"
                            }
                        ]
                    }

  ]
}
